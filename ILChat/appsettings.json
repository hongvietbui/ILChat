{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "SqlServer": "{SQL_SERVER_CONNECTION_STRING}",
    "MongoDb": "{MONGO_DB_CONNECTION_STRING}"
  }
}
