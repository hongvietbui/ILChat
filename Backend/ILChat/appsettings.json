{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "SqlServer": "{SQL_SERVER_CONNECTION_STRING}",
    "MongoDb": "{MONGO_DB_CONNECTION_STRING}"
  },
  "KeyCloak": {
    "Realm": "{KEYCLOAK_REALM}",
    "ClientId": "{KEYCLOAK_CLIENT_ID}",
    "ValidateIssuer": "{KEYCLOAK_VALIDATE_ISSUER}",
    "BaseUrl": "{KEYCLOAK_BASE_URL}",
    "ClientSecret": "{KEYCLOAK_CLIENT_SECRET}"
  }
}
